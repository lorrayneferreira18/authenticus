{
  "expo": {
    "name": "Authenticus",
    "slug": "Authenticus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoNome.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.app.Authenticus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoNome.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.app.Authenticus"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Authenticus to access your camera",
          "microphonePermission": "Allow Authenticus to access your mircophone",
          "recordAudioAndroid": true
        }
      ]
    ]
  }
}
